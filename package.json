{
  "name": "protoc-gen-swagger-node",
  "version": "0.1.0",
  "description": "A wrapper around the gRPC swagger definitions generator for Node.js",
  "scripts": {
    "postinstall": "go-npm install",
    "preuninstall": "go-npm uninstall"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iloginow/protoc-gen-swagger-node.git"
  },
  "keywords": [
    "protoc",
    "swagger",
    "node"
  ],
  "author": "Ilia Loginov <i.loginow@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iloginow/protoc-gen-swagger-node/issues"
  },
  "dependencies": {
    "go-npm": "^0.1.9"
  },
  "goBinary": {
    "name": "protoc-gen-swagger",
    "path": "./bin",
    "url": "https://github.com/iloginow/protoc-gen-swagger/releases/download/v{{version}}/protoc-gen-swagger_{{version}}_{{platform}}_{{arch}}.tar.gz"
  }
}
